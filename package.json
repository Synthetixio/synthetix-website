{
	"name": "synthetix-website",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"postbuild": "if [[ ${BUILD_SITEMAP} ]]; then next-sitemap --config next-sitemap.js; fi",
		"start": "next start --port 8888",
		"start:tunnel": "ngrok http 8888",
		"svg": "npx svgr -d src/svg src/svg --ext tsx --template src/utils/svgTemplate.ts",
		"analyse:bundle": "ANALYZE_BUNDLE=true npm start",
		"format": "prettier --write \"**/*.{ts,tsx}\"",
		"lint": "eslint . --ext ts,.tsx",
		"lint:fix": "eslint . --ext ts,.tsx --fix",
		"lint:sarif": "node tests/lint.js . --ext ts,.tsx && tsc",
		"tsc": "tsc --noEmit"
	},
	"dependencies": {
		"@socialgouv/matomo-next": "1.2.2",
		"@synthetixio/contracts-interface": "2.49.0",
		"axios": "0.20.0",
		"bignumber.js": "9.0.1",
		"ethers": "5.1.0",
		"ip-range-check": "0.2.0",
		"lodash": "4.17.21",
		"next": "9.5.5",
		"numeral": "2.0.6",
		"react": "16.13.1",
		"react-dom": "16.13.1",
		"react-fastclick": "3.0.2",
		"react-hamburger-menu": "1.2.1",
		"react-query": "3.13.5",
		"react-transition-group": "4.4.1",
		"smoothscroll-polyfill": "^0.4.4",
		"styled-components": "5.1.1",
		"styled-media-query": "2.1.2",
		"styled-reset": "4.2.2",
		"uuid": "^8.3.2"
	},
	"devDependencies": {
		"@microsoft/eslint-formatter-sarif": "2.1.5",
		"@next/bundle-analyzer": "9.4.1",
		"@svgr/cli": "5.4.0",
		"@types/lodash": "4.14.168",
		"@types/numeral": "0.0.28",
		"@types/react": "16.9.35",
		"@types/react-hamburger-menu": "0.0.3",
		"@types/react-responsive": "^8.0.2",
		"@types/react-slick": "0.23.4",
		"@types/react-transition-group": "4.4.0",
		"@types/styled-components": "^5.1.2",
		"@types/uuid": "^8.3.1",
		"@types/webfontloader": "1.6.30",
		"@types/webpack-env": "1.15.2",
		"@typescript-eslint/eslint-plugin": "2.33.0",
		"@typescript-eslint/parser": "2.33.0",
		"@zeit/next-source-maps": "0.0.4-canary.1",
		"babel-eslint": "^10.1.0",
		"babel-plugin-styled-components": "^1.11.1",
		"cross-env": "7.0.2",
		"cypress": "4.8.0",
		"del-cli": "3.0.1",
		"dotenv": "8.2.0",
		"eslint": "7.0.0",
		"eslint-config-prettier": "6.10.1",
		"eslint-config-react-app": "5.2.1",
		"eslint-plugin-flowtype": "4.7.0",
		"eslint-plugin-import": "2.22.0",
		"eslint-plugin-jsx-a11y": "6.3.1",
		"eslint-plugin-prettier": "3.1.4",
		"eslint-plugin-react": "7.20.6",
		"eslint-plugin-react-hooks": "2.5.1",
		"next-sitemap": "1.3.2",
		"ngrok": "^3.2.7",
		"now": "19.1.0",
		"prettier": "^2.1.2",
		"typescript": "^3.9.7",
		"version-bump-prompt": "6.0.3"
	}
