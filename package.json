{
	"name": "synthetix-website",
	"version": "1.3.1",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"postbuild": "if [[ ${BUILD_SITEMAP} ]]; then next-sitemap --config next-sitemap.js; fi",
		"start": "next start --port 8888",
		"svg": "npx svgr -d src/svg src/svg --ext tsx --template src/utils/svgTemplate.ts",
		"format": "prettier --write \"**/*.{ts,tsx}\"",
		"lint": "eslint . --ext ts,.tsx",
		"lint:fix": "eslint . --ext ts,.tsx --fix",
		"lint:sarif": "node ./tests/lint.js . --ext ts,.tsx && tsc",
		"tsc": "tsc --noEmit",
		"test": "jest",
		"e2e": "sudo SKIP_METAMASK_SETUP=true SKIP_METAMASK_INSTALL=true synpress open",
		"e2e:run": "SKIP_METAMASK_SETUP=true SKIP_METAMASK_INSTALL=true synpress run",
		"prepare": "husky install"
	},
	"dependencies": {
		"@chakra-ui/react": "^2.3.1",
		"@emotion/react": "^11.10.4",
		"@emotion/styled": "^11.10.4",
