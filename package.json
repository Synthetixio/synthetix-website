{
	"name": "synthetix-website",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"postbuild": "if [[ ${BUILD_SITEMAP} ]]; then next-sitemap --config next-sitemap.js; fi",
		"start": "next start --port 8888",
		"svg": "npx svgr -d src/svg src/svg --ext tsx --template src/utils/svgTemplate.ts",
		"format": "prettier --write \"**/*.{ts,tsx}\"",
		"lint": "eslint . --ext ts,.tsx",
		"lint:fix": "eslint . --ext ts,.tsx --fix",
		"lint:sarif": "node ./tests/lint.js . --ext ts,.tsx && tsc",
		"tsc": "tsc --noEmit",
		"e2e": "SKIP_METAMASK_SETUP=true synpress open",
		"e2e:run": "SKIP_METAMASK_SETUP=true synpress run"
	},
	"dependencies": {
		"@socialgouv/matomo-next": "1.2.2",
		"@synthetixio/contracts-interface": "2.49.0",
		"axios": "0.23.0",
		"bignumber.js": "9.0.1",
		"ethers": "5.1.0",
		"ip-range-check": "0.2.0",
		"lodash": "4.17.21",
		"next": "11.1.2",
		"numeral": "2.0.6",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"react-fastclick": "3.0.2",
		"react-hamburger-menu": "1.2.1",
		"react-query": "3.13.5",
		"react-transition-group": "4.4.1",
		"smoothscroll-polyfill": "^0.4.4",
		"styled-components": "5.1.1",
		"styled-media-query": "2.1.2",
		"styled-reset": "4.2.2"
	},
	"devDependencies": {
		"@babel/core": "^7.8.0",
		"@microsoft/eslint-formatter-sarif": "2.1.5",
		"@svgr/cli": "^5.5.0",
		"@synthetixio/synpress": "0.9.25-beta.23",
		"@types/lodash": "^4.14.168",
		"@types/numeral": "^0.0.28",
		"@types/react": "^17.0.2",
		"@types/react-hamburger-menu": "^0.0.3",
		"@types/react-responsive": "^8.0.2",
		"@types/react-slick": "^0.23.4",
		"@types/react-transition-group": "^4.4.0",
		"@types/styled-components": "^5.1.2",
		"@types/webfontloader": "^1.6.30",
		"@types/webpack-env": "^1.15.2",
		"@typescript-eslint/eslint-plugin": "^4.0.0",
		"@typescript-eslint/parser": "^4.0.0",
		"babel-eslint": "^10.1.0",
		"babel-plugin-styled-components": "^1.13.2",
		"eslint": "^7.5.0",
		"eslint-config-prettier": "6.10.1",
		"eslint-config-react-app": "^6.0.0",
		"eslint-plugin-flowtype": "^5.2.0",
		"eslint-plugin-import": "^2.22.0",
		"eslint-plugin-jsx-a11y": "^6.3.1",
		"eslint-plugin-prettier": "3.1.4",
		"eslint-plugin-react": "^7.20.3",
		"eslint-plugin-react-hooks": "^4.0.8",
		"next-sitemap": "1.3.2",
		"prettier": "^2.1.2",
		"prop-types": "^15.7.2",
		"react-is": "^17.0.2",
		"typescript": "^4.4.4",
		"version-bump-prompt": "6.1.0"
	}
}
