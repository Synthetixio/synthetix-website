		"@sanity/client": "^3.1.0",
		"@sanity/image-url": "^1.0.1",
		"@socialgouv/matomo-next": "1.2.2",
		"@synthetixio/contracts-interface": "^2.74.3",
		"@synthetixio/ui": "^1.24.1",
		"@synthetixio/v3-theme": "^1.0.2-0",
		"@types/react-instantsearch-dom": "^6.12.2",
		"@vercel/node": "^1.14.0",
		"algoliasearch": "^4.13.0",
		"axios": "0.23.0",
		"bignumber.js": "9.0.1",
		"ethers": "5.6.9",
		"framer-motion": "^7.2.1",
		"jsonpath": "^1.1.1",
		"lodash": "4.17.21",
		"next": "12.0.4",
		"next-sanity": "^0.5.0",
		"next-sanity-image": "^3.2.1",
		"numeral": "2.0.6",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-hamburger-menu": "1.2.1",
		"react-icons": "^4.3.1",
		"react-instantsearch-dom": "^6.23.1",
		"react-multi-carousel": "^2.8.0",
		"react-player": "^2.9.0",
		"react-responsive-carousel": "^3.2.23",
		"react-responsive-modal": "^6.2.0",
		"react-scrollspy": "^3.4.3",
		"react-syntax-highlighter": "^15.4.5",
		"react-transition-group": "4.4.1",
		"sanity-algolia": "^1.0.2",
		"slugify": "^1.6.5",
		"smoothscroll-polyfill": "^0.4.4",
		"styled-components": "5.1.1",
		"styled-media-query": "2.1.2",
		"styled-reset": "4.2.2"
	},
	"devDependencies": {
		"@babel/core": "^7.8.0",
		"@microsoft/eslint-formatter-sarif": "2.1.5",
		"@next/eslint-plugin-next": "^12.1.0",
		"@svgr/cli": "^5.5.0",
		"@synthetixio/synpress": "^1.1.1",
		"@testing-library/dom": "^8.11.0",
